{
    "name": "hauerheinrich/tt-address-places",
    "type": "typo3-cms-extension",
    "description": "Wrapper for ***",
    "authors": [
        {
            "name": "Christian Hackl",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4 || ^10.5",
        "friendsoftypo3/tt-address": ""
    },
    "require-dev": {
        "typo3/testing-framework": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\TtAddressPlaces\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HauerHeinrich\\TtAddressPlaces\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/tt-address-places": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "tt_address_places"
        }
    }
}
